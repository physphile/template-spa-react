{
	"name": "template-spa-react",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint . --quiet",
		"preview": "vite preview",
		"lint:fix": "eslint . --fix",
		"stylelint": "stylelint \"**/*.css\" --quiet",
		"stylelint:fix": "stylelint \"**/*.css\" --fix",
		"typecheck": "tsc -b",
		"circular": "npx dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform \"./src/main.tsx\"",
		"deadcode": "knip --exclude binaries,dependencies,unlisted",
		"check": "run-p typecheck lint stylelint circular deadcode build",
		"analyze": "ANALYZE=true vite build",
		"prepare": "husky"
	},
	"dependencies": {
		"@gravity-ui/date-utils": "^2.6.0",
		"@gravity-ui/navigation": "^3.11.0",
		"@gravity-ui/table": "^1.13.0",
		"@tanstack/react-query": "^5.90.12",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.69.0",
		"use-query-params": "^2.2.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@gravity-ui/components": "^4.17.0",
		"@gravity-ui/date-components": "^3.4.1",
		"@gravity-ui/icons": "^2.16.0",
		"@gravity-ui/illustrations": "^2.1.0",
		"@gravity-ui/uikit": "^7.29.0",
		"@hey-api/openapi-ts": "^0.89.2",
		"@stylistic/stylelint-plugin": "^4.0.0",
		"@types/node": "^24.10.3",
		"@types/react": "^18.3.27",
		"@types/react-dom": "^18.3.7",
		"@vitejs/plugin-react": "^5.1.2",
		"dpdm": "^3.14.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-unicorn": "^60.0.0",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"jiti": "^2.6.1",
		"knip": "^5.77.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.7.4",
		"react-router": "^7.11.0",
		"sass-embedded": "^1.97.1",
		"stylelint": "^16.26.1",
		"stylelint-config-recess-order": "^7.4.0",
		"stylelint-config-recommended": "^17.0.0",
		"stylelint-high-performance-animation": "^1.11.0",
		"stylelint-order": "^7.0.0",
		"stylelint-plugin-logical-css": "^1.2.3",
		"stylelint-plugin-use-baseline": "^1.1.5",
		"stylelint-use-nesting": "^6.0.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1",
		"usehooks-ts": "^3.1.1",
		"vite": "npm:rolldown-vite@^7.2.11",
		"vite-bundle-analyzer": "^1.3.2"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@^7.2.11"
	}
}
