		"@tanstack/react-query": "^5.85.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.62.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@gravity-ui/components": "^4.11.0",
		"@gravity-ui/date-components": "^3.2.4",
		"@gravity-ui/icons": "^2.16.0",
		"@gravity-ui/illustrations": "^2.1.0",
		"@gravity-ui/uikit": "^7.20.0",
		"@hey-api/openapi-ts": "^0.80.14",
