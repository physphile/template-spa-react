{
	"name": "template-spa-react",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint . --quiet",
		"preview": "vite preview",
		"lint:fix": "eslint . --fix",
		"format:fix": "prettier --write . --log-level warn",
		"format": "prettier --check . --log-level warn",
		"typecheck": "tsc --noEmit",
		"circular": "npx dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform \"./src/main.tsx\"",
		"deadcode": "knip --exclude binaries,dependencies,unlisted",
		"check": "run-p typecheck lint format circular deadcode"
	},
	"dependencies": {
		"@gravity-ui/date-utils": "^2.6.0",
		"@gravity-ui/navigation": "^3.10.1",
		"@gravity-ui/table": "^1.13.0",
		"@tanstack/react-query": "^5.90.11",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.66.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@gravity-ui/components": "^4.16.0",
		"@gravity-ui/date-components": "^3.4.1",
		"@gravity-ui/icons": "^2.16.0",
		"@gravity-ui/illustrations": "^2.1.0",
		"@gravity-ui/uikit": "^7.26.1",
		"@hey-api/openapi-ts": "^0.87.5",
		"@types/node": "^24.10.1",
		"@types/react": "^18.3.27",
		"@types/react-dom": "^18.3.7",
		"@vitejs/plugin-react-swc": "^4.2.2",
		"dpdm": "^3.14.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.24",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-unicorn": "^60.0.0",
		"globals": "^16.5.0",
		"jiti": "^2.6.1",
		"knip": "^5.70.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.7.1",
		"react-router": "^7.9.6",
		"sass-embedded": "^1.93.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0",
		"vite": "npm:rolldown-vite@^7.2.8"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@^7.2.8"
	}
}
